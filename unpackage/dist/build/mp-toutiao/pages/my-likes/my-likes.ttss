page{height:100%;display:flex}.my-likes{display:flex;flex-direction:column;flex:1}.my-likes .null{text-align:center;margin:20rpx auto;font-size:35rpx;color:#d9d9d9}.my-likes .scroll{flex:1;overflow:hidden;box-sizing:border-box}.my-likes .scroll .scroll-list{height:100%;display:flex;flex-direction:column}.my-likes .scroll .scroll-list .likes-item{position:relative;box-shadow:rgba(0,0,0,.35) 0 5px 15px;border-radius:20rpx;padding:30rpx;margin:20rpx;font-size:35rpx;color:#fff}